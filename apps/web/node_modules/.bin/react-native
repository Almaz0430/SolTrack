#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/react-native@0.81.4_@babel+_61190940d38793c86cde286a29521862/node_modules/react-native/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/react-native@0.81.4_@babel+_61190940d38793c86cde286a29521862/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/react-native@0.81.4_@babel+_61190940d38793c86cde286a29521862/node_modules/react-native/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/react-native@0.81.4_@babel+_61190940d38793c86cde286a29521862/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/react-native@0.81.4_@babel+_61190940d38793c86cde286a29521862/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/react-native@0.81.4_@babel+_61190940d38793c86cde286a29521862/node_modules/react-native/cli.js" "$@"
fi
