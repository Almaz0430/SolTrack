#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/next@15.5.3_@babel+core@7.2_7229a6f3c8984d47e457499a8a3f8b03/node_modules/next/dist/bin/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/next@15.5.3_@babel+core@7.2_7229a6f3c8984d47e457499a8a3f8b03/node_modules/next/dist/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/next@15.5.3_@babel+core@7.2_7229a6f3c8984d47e457499a8a3f8b03/node_modules/next/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/next@15.5.3_@babel+core@7.2_7229a6f3c8984d47e457499a8a3f8b03/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/next@15.5.3_@babel+core@7.2_7229a6f3c8984d47e457499a8a3f8b03/node_modules/next/dist/bin/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/next@15.5.3_@babel+core@7.2_7229a6f3c8984d47e457499a8a3f8b03/node_modules/next/dist/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/next@15.5.3_@babel+core@7.2_7229a6f3c8984d47e457499a8a3f8b03/node_modules/next/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/next@15.5.3_@babel+core@7.2_7229a6f3c8984d47e457499a8a3f8b03/node_modules:/mnt/c/Users/Almaz/Desktop/hacksolana/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/next" "$@"
else
  exec node  "$basedir/../../dist/bin/next" "$@"
fi
